@model IList<string>

@{
    ViewBag.Title = "Names";
}

<h2>Names</h2>

@if (Model.Count == 0)
{
    using (Html.BeginForm())
    {
        for (int i = 0; i < 3; i++)
        {
            <div><label>@(i + 1):</label>@Html.TextBox("names")</div>
        }
        <button type="submit">提交</button>
    }
}
else
{
    foreach (string str in Model)
    {
        <p>@str</p>
    }
    @Html.ActionLink("返回", "Names")
}
